{
  "name": "nestjs-base-proyect",
  "version": "0.0.1",
  "description": "Base proyect for nestJs+graphql app",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:gen": "npx prisma generate --schema ./prisma/schema.prisma",
    "prisma:pull": "npx prisma db pull --schema ./prisma/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --name init --schema ./prisma/schema.prisma",
    "docker": "docker build -t base-nestjs-app -f Dockerfile ."
  },
  "dependencies": {
    "@nestjs/apollo": "10.1.7",
    "@nestjs/common": "9.2.1",
    "@nestjs/core": "9.2.1",
    "@nestjs/graphql": "10.1.7",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "9.2.1",
    "@prisma/client": "4.8.1",
    "apollo-server-express": "3.11.1",
    "class-transformer": "^0.5.1",
    "class-validator": "0.14.0",
    "graphql": "16.6.0",
    "graphql-tools": "8.3.15",
    "nest-winston": "^1.8.0",
    "nestjs-prisma-graphql-crud-gen": "^1.0.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "4.0.4",
    "rxjs": "7.8.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "9.1.8",
    "@nestjs/schematics": "9.0.4",
    "@nestjs/testing": "9.2.1",
    "@types/express": "4.17.15",
    "@types/jest": "29.2.5",
    "@types/node": "18.11.18",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.3.1",
    "prettier": "2.8.2",
    "prisma": "4.8.1",
    "prisma-class-generator": "^0.2.5",
    "prisma-nestjs-graphql": "^17.1.0",
    "supertest": "6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.9.4"
  },
  "keywords": [
    "NestJS",
    "Prisma",
    "Apollo-Server",
    "Graphql"
  ],
  "author": "Jhony Benitez",
  "license": "ISC"
}
